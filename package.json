{
    "name": "mui-wksp-catalog",
    "version": "0.1.1",
    "private": true,
    "env": {
        "WORKSPACE": "~wrg/workspaces/mui-wksp-catalog"
    },
    "scripts": {
        "typecheck": ". ~/.zshrc; type tsc; echo $PATH; echo ' ' ; tsc --noEmit",
        "build": ". ~/.zshrc; echo SHELL=$SHELL PATH=$PATH; type tsx tsc; tsc -b && vite build",
        "dev": "DEBUG=yes webpack --serve",
        "lint": "eslint .",
        "pack": "webpack",
        "clean:deb-pack": "rm -rf mui-wksp-catalog-try2; mkdir -p mui-wksp-catalog-try2/test; pwd; cp -pr house_gitstyle.css index*.html package.json README.md images test views mui-wksp-catalog-try2",
        "build:deb-pack": ". ~/.zshrc; tsc; rsync -rlt src/*.js* test mui-wksp-catalog-try2/",
        "preview": "vite preview",
        "serve": "npx webpack serve",
        "pyserve": "cd mui-wksp-catalog-try2; python3 -m http.server 4001"
    },
    "browserslist": [
        "fully supports es6-module"
    ],
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "overrides": {
        "glob": "10.4.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@rauschma/stringio": "^1.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/express-session": "^1.18.1",
        "@types/jest": "^29.5.12",
        "@types/luxon": "^3.4.2",
        "@types/markdown-it": "^14.0.1",
        "@types/pug": "^2.0.10",
        "@types/react": "^18.0.31",
        "@types/react-dom": "^18.0.11",
        "@types/watch": "^1.0.6",
        "babel-plugin-named-exports-order": "^0.0.2",
        "body-parser": "^2.2.0",
        "common-es": "^1.0.1",
        "ejs": "^3.1.10",
        "env-cmd": "^10.1.0",
        "eslint": "^9.24.0",
        "express-session": "^1.18.1",
        "file-loader": "^6.2.0",
        "globals": "^16.0.0",
        "glvlib": "file:local_package_overrides/glvlib",
        "html-loader": "^5.0.0",
        "html-webpack-plugin": "^5.6.3",
        "jest": "^29.7.0",
